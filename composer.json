{
    "name": "wink/model-generator",
    "description": "Laravel model generator supporting MySQL and SQLite",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Wingerd",
            "email": "github@wingerd.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/console": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.36",
        "pestphp/pest-plugin": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Wink\\ModelGenerator\\": "src/",
            "Wink\\ModelGenerator\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wink\\ModelGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wink\\ModelGenerator\\ModelGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
